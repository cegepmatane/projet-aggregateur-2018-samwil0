<Window x:Class="ProjetAgregation.Fenetres.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetAgregation.Fenetres"
        mc:Ignorable="d"
        Title="Agregation" Height="450" Width="800" Background="Red">
    <Grid Background="Black">
        <Grid HorizontalAlignment="Left" Height="428" VerticalAlignment="Top" Width="55">
            <Grid.RowDefinitions>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
            </Grid.RowDefinitions>
            <Button Content="Menu" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Goldenrod" Click="Menu_Click"/>
            <Button Content="Acueil" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Goldenrod" Click="Acueil_Click" Grid.Row="1"/>
            <Button Content="Amis" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Red" Click="Amis_Click" Grid.Row="2"/>
            <Button Content="Email" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Goldenrod" Click="Email_Click" Grid.Row="3"/>
            <Button Content="Graph" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Goldenrod" Click="Graph_Click" Grid.Row="4"/>
            <Button Content="News" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Goldenrod" Click="News_Click" Grid.Row="5"/>
            <Button Content="Chat" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Goldenrod" Click="Chat_Click" Grid.Row="6"/>
            <Button Content="Pokemon" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Goldenrod" Click="Pokemon_Click" Grid.Row="7"/>
            <Button Content="Crypto" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40" Height="40" Background="Goldenrod" Click="Crypto_Click" Grid.Row="8"/>
        </Grid>

        <Grid HorizontalAlignment="Left" Height="63" Margin="55,0,0,0" VerticalAlignment="Top" Width="730">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="250*"/>
                <ColumnDefinition Width="250*"/>
                <ColumnDefinition Width="250*"/>
            </Grid.ColumnDefinitions>

            <TextBox HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Amis" VerticalAlignment="Center" Width="120" Background="Goldenrod" FontSize="15" TextAlignment="Center" IsReadOnly = "True" Grid.Column="1"/>
            <TextBox HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="AAAA/MM/JJ" VerticalAlignment="Center" Width="120" Background="Goldenrod" FontSize="15" TextAlignment="Center" IsReadOnly = "True" Grid.Column="0"/>
            <TextBox HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Utulisateur" VerticalAlignment="Center" Width="120" Background="Goldenrod" FontSize="15" TextAlignment="Center" IsReadOnly = "True" Grid.Column="2"/>
        </Grid>

        <Grid HorizontalAlignment="Left" Height="355" Margin="55,63,0,0" VerticalAlignment="Top" Width="730" Background="Red">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="150*"/>
                <ColumnDefinition Width="150*"/>
                <ColumnDefinition Width="150*"/>
                <ColumnDefinition Width="150*"/>
                <ColumnDefinition Width="150*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="200*"/>
                <RowDefinition Height="200*"/>
            </Grid.RowDefinitions>
            <TextBox Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="1" Grid.Row="0"  HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="1" Grid.Row="0"  HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="2" Grid.Row="0"  HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="2" Grid.Row="0" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="2" Grid.Row="0" HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="3" Grid.Row="0"  HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="3" Grid.Row="0" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="3" Grid.Row="0"  HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="4" Grid.Row="0"  HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="4" Grid.Row="0" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="4" Grid.Row="0"  HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="0" Grid.Row="1"  HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="0" Grid.Row="1" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="0" Grid.Row="1"  HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="1" Grid.Row="1"  HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="1" Grid.Row="1"  HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="2" Grid.Row="1"  HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="3" Grid.Row="1" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="3" Grid.Row="1" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="3" Grid.Row="1"  HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />

            <TextBox Grid.Column="4" Grid.Row="1" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Nom" VerticalAlignment="Center" Width="120" Margin="0,0,0.0,0" TextAlignment="Center"/>
            <TextBox Grid.Column="4" Grid.Row="1" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="Email" VerticalAlignment="Center" Width="120" Margin="0,50,0.0,0" TextAlignment="Center"/>
            <Image Grid.Column="4" Grid.Row="1"  HorizontalAlignment="Center" Height="50" VerticalAlignment="Center" Width="50" Margin="-50,-150,0,0.0" />
        </Grid>

    </Grid>
</Window>
